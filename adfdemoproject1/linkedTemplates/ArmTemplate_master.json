{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdemoproject1"
		},
		"ADLS_ProjectDemo_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_ProjectDemo_LS'"
		},
		"MongoDB_money_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDB_money_LS'"
		},
		"MongoDb_Bank_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDb_Bank_LS'"
		},
		"MongoDb_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDb_LS'"
		},
		"ADLS_ProjectDemo_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiteshadls.dfs.core.windows.net/"
		},
		"MongoDB_money_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "money_laundering"
		},
		"MongoDb_Bank_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Bank_DB"
		},
		"MongoDb_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "projectdemo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemoproject1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_ProjectDemo_LS_accountKey": {
						"value": "[parameters('ADLS_ProjectDemo_LS_accountKey')]"
					},
					"MongoDB_money_LS_connectionString": {
						"value": "[parameters('MongoDB_money_LS_connectionString')]"
					},
					"MongoDb_Bank_LS_connectionString": {
						"value": "[parameters('MongoDb_Bank_LS_connectionString')]"
					},
					"MongoDb_LS_connectionString": {
						"value": "[parameters('MongoDb_LS_connectionString')]"
					},
					"ADLS_ProjectDemo_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_ProjectDemo_LS_properties_typeProperties_url')]"
					},
					"MongoDB_money_LS_properties_typeProperties_database": {
						"value": "[parameters('MongoDB_money_LS_properties_typeProperties_database')]"
					},
					"MongoDb_Bank_LS_properties_typeProperties_database": {
						"value": "[parameters('MongoDb_Bank_LS_properties_typeProperties_database')]"
					},
					"MongoDb_LS_properties_typeProperties_database": {
						"value": "[parameters('MongoDb_LS_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfdemoproject1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfdemoproject1_ArmTemplate_0')]"
			]
		}
	]
}